{
  "name": "mass.js",
  "version": "1.3.2",
  "description": "ES Module for parsing and formatting mass units.",
  "homepage": "https://mass.tylervigario.com",
  "module": "./lib/entries/US.mjs",
  "type": "module",
  "exports": {
    "./entry/US": "./lib/entries/US.mjs",
    "./entry/UK": "./lib/entries/UK.mjs",
    "./entry/SI": "./lib/entries/SI.mjs"
  },
  "scripts": {
    "lint": "eslint --config ./.eslintrc.json ./**/*.mjs",
    "test": "ava ./test/tests.mjs --verbose",
    "test:coverage": "c8 ava ./test/tests.mjs",
    "performance": "node --experimental-modules ./performance/index.mjs"
  },
  "keywords": [
    "mass",
    "unit",
    "imperial",
    "metric",
    "ton",
    "pound",
    "ounce",
    "weight"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeekLogic/Mass.js"
  },
  "bugs": {
    "url": "https://github.com/MeekLogic/Mass.js/issues"
  },
  "author": {
    "name": "MeekLogic (Tyler Vigario)",
    "email": "tylervigario90@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "c8": "^7.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "microtime": "^3.0.0"
  },
  "dependencies": {
    "n2words": "^1.6.2"
  },
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
