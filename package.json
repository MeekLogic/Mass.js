{
  "name": "mass.js",
  "version": "1.2.5",
  "description": "ES Module for parsing and formatting mass units.",
  "homepage": "https://mass.tylervigario.com",
  "module": "./src/US.js",
  "scripts": {
    "lint": "eslint --config ./.eslintrc.js ./src",
    "test": "ava ./tests/index.js --verbose",
    "test:coverage": "nyc ava ./tests/index.js",
    "performance": "node -r esm ./performance/index.js"
  },
  "keywords": [
    "mass",
    "unit",
    "imperial",
    "metric",
    "ton",
    "pound",
    "ounce",
    "weight"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeekLogic/Mass.js"
  },
  "bugs": {
    "url": "https://github.com/MeekLogic/Mass.js/issues"
  },
  "author": "MeekLogic (Tyler Vigario)",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "ava": "^3.5.0",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "microtime": "^3.0.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "written-number": "^0.9.1"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
