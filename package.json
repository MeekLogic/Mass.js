{
  "name": "mass.js",
  "version": "1.1.1",
  "description": "JavaScript library for parsing and formatting mass units.",
  "homepage": "https://meeklogic.github.io/Mass.js/",
  "main": "./src/US.js",
  "scripts": {
    "lint": "eslint --config ./.eslintrc.js ./src",
    "tests": "ava ./tests/index.js --verbose",
    "tests:coverage": "nyc --reporter=lcov --reporter=text-summary ava ./tests/index.js",
    "performance": "node -r esm ./performance/index.js",
    "docs": "npm run documentation",
    "documentation": "esdoc"
  },
  "keywords": [
    "mass",
    "unit",
    "imperial",
    "metric",
    "ton",
    "pound",
    "ounce",
    "weight"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeekLogic/Mass.js"
  },
  "bugs": {
    "url": "https://github.com/MeekLogic/Mass.js/issues"
  },
  "author": "MeekLogic (Tyler Vigario)",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "ava": "^2.4.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.6.0",
    "esm": "^3.2.25",
    "microtime": "^3.0.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {},
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "esm"
    ]
  }
}
